{
    "problemMatcher": [
        {
            "owner": "lint-generic-spelling",
            "pattern": [
                {
                    "regexp": "^(notice|error)(:(file|dir):([^:]+)(:lines? (\\d+)(-(\\d+))?)?)?: (.+)$",
                    "severity": 1,
                    "file": 4,
                    "line": 6,
                    "message": 9
                }
            ]
        }
    ]
}
